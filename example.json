{
    "name": {
        "type": "text",
        "size": 50,
        "required": true,
        "placeholder": "Entrez votre nom"
    },
    "age": {
        "type": "number",
        "required": false,
        "min": 1,
        "max": 100
    },
    "file": {
        "type": "file",
        "size": 10,
        "ext": ["png", "jpeg", "pdf"],
        "required": true
    },
    "date": {
        "type": "date",
        "format": "fr-FR",
        "required": false
    }
}
